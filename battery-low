#lxpanel

Plugin {
    type = batt
    Config {
        HideIfNoBattery=0
        AlarmCommand=xmessage Battery low
        AlarmTime=5
        BackgroundColor=black
        BorderWidth=1
        ChargingColor1=#28f200
        ChargingColor2=#22cc00
        DischargingColor1=#ffee00
        DischargingColor2=#d9ca00
        Size=8
    }
}

